{
	"extends": ["airbnb", "prettier", "prettier/react"],
	"plugins": ["prettier"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"prettier/prettier": "error",
		"linebreak-style": 0,
		"react/prefer-stateless-function": 0,
		"react/prop-types": 0,
		"no-console": 0,
		"no-restricted-syntax": 0,
		"no-else-return": 0
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
